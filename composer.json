{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "annaghd/php-credit-card-validator-plus": "dev-master",
        "arielmejiadev/larapex-charts": "^1.0",
        "cartalyst/stripe-laravel": "^11.0",
        "divineomega/laravel-offensive-validation-rule": "^1.2",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "google/apiclient": "^2.0",
        "laravel-notification-channels/facebook-poster": "^2.0",
        "laravel-notification-channels/twitter": "^3.0",
        "laravel/framework": "^6.2",
        "laravel/horizon": "^3.7",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "league/flysystem-sftp": "~1.0",
        "linkthrow/ffmpeg": "dev-master",
        "predis/predis": "^1.1",
        "socialiteproviders/discord": "^2.0",
        "socialiteproviders/facebook": "^1.0",
        "socialiteproviders/twitter": "^3.0",
        "stripe/stripe-php": "^7.25",
        "thomaswelton/laravel-gravatar": "~1.0",
        "thujohn/twitter": "^2.2",
        "yajra/laravel-datatables": "1.5",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "laravel/telescope": "^3.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/HelpersClass/ListUtils.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
